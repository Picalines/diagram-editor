{
	"name": "web",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "vite dev",
		"build": "vite build",
		"codegen": "npm run codegen:api",
		"codegen:api": "npm run codegen:api:cleanup && npm run codegen:api:generate",
		"codegen:api:cleanup": "rm -rf src/api/codegen",
		"codegen:api:generate": "openapi --input src/api/swagger.json --output src/api/codegen --client axios --name GeneratedApiClient --useOptions --useUnionTypes",
		"preview": "vite preview",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --plugin-search-dir . --check . && eslint .",
		"format": "prettier --plugin-search-dir . --write ."
	},
	"devDependencies": {
		"@fontsource/fira-mono": "^4.5.10",
		"@neoconfetti/svelte": "^1.0.0",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.20.4",
		"@types/cookie": "^0.5.1",
		"@types/node": "^20.8.10",
		"@typescript-eslint/eslint-plugin": "^6.0.0",
		"@typescript-eslint/parser": "^6.0.0",
		"eslint": "^8.28.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-index": "^1.1.7",
		"eslint-plugin-svelte": "^2.30.0",
		"form-data": "^4.0.0",
		"openapi-typescript-codegen": "^0.25.0",
		"prettier": "^3.0.3",
		"prettier-plugin-svelte": "^3.0.3",
		"svelte": "^4.0.5",
		"svelte-check": "^3.4.3",
		"tslib": "^2.4.1",
		"typescript": "^5.0.0",
		"vite": "^4.4.2"
	},
	"dependencies": {
		"axios": "^1.6.2",
		"purify-ts": "^2.0.1"
	}
}
